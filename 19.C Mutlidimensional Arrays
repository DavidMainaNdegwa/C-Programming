SYNTAX
int arr[2][3];
//There will be 2 arrays and 3 elements in each array

EXAMPLE
#include <stdio.h>

int main() {

int arr[2][3] = { {2,4,5} , {1,3,6} };

printf("%d",arr[0][0]);
printf("%d",arr[1][2]);

return 0;
}

OUTPUT
2
6

FORLOOP
#include <stdio.h>

int main() {

int arr[2][3] = { {2,4,5} , {1,3,6} };

for(int i = 0; i < 2; i++) {
for(int j = 0; j < 3; j++){
printf("%d ",arr[i][j]);
}
}

return 0;
}

OUTPUT
2 4 5 1 3 6
